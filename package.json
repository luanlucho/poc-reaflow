{
  "name": "next_boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "yarn start",
    "build": "next build",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern !cypress/.eslintrc.js --ext .js,jsx,.ts,.tsx .",
    "test": "jest --watchAll",
    "test:ci": "cross-env CI=true jest --runInBand",
    "test:all": "yarn test --watchAll=false --coverage",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json|css|scss|html)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "deploy": "npx vercel"
  },
  "dependencies": {
    "@bugsnag/js": "^7.16.2",
    "@bugsnag/plugin-react": "^7.16.2",
    "axios": "^0.26.1",
    "faker": "^5.5.3",
    "next": "^12.1.3",
    "polished": "^4.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-error-boundary": "^3.1.4",
    "react-query": "^3.34.16",
    "reaflow": "^5.0.4",
    "styled-components": "^5.3.3"
  },
  "workspaces": [
    "."
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.22",
    "@types/react": "17.0.42",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-jest": "^27.5.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.0.8",
    "next-transpile-modules": "^9.0.0",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.4",
    "turbo": "^1.1.10",
    "typescript": "4.6.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
